<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>ユーザー名</th>
      <th>メール</th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.id %></td>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>